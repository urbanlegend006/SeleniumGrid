package PrallelTestThreads;

import org.testng.annotations.Test;

public class Test1 extends base {

	@Test
	public void add1() throws InterruptedException {
		Thread.sleep(1);
		for (int j = 0; j < 100; j++) {
			int b = base.getInt();
			int k = ++b;
			System.out.println("Test1 = " + k);
			base.setInt(k);
		}
	}
}
